<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
            <section name="Ponto.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
        </sectionGroup>
    </configSections>
    <connectionStrings>
        <add name="Ponto.Properties.Settings.colibri8ConnectionString"
            connectionString="Data Source=MQM-HOME\SQLEXPRESS;Initial Catalog=colibri8;Integrated Security=True"
            providerName="System.Data.SqlClient" />
        <add name="Ponto.Properties.Settings.dbPontoConnectionString"
            connectionString="Provider=Microsoft.Jet.OLEDB.4.0;Data Source=|DataDirectory|\dbPonto.mdb"
            providerName="System.Data.OleDb" />
    </connectionStrings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <userSettings>
        <Ponto.Properties.Settings>
            <setting name="PrinterName" serializeAs="String">
                <value>EPSON</value>
            </setting>
            <setting name="CanPrint" serializeAs="String">
                <value>True</value>
            </setting>
        </Ponto.Properties.Settings>
    </userSettings>
</configuration>